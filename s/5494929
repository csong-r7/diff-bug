          <bar:RegKeyExists Key="HKEY_LOCAL_MACHINE" Subkey="SYSTEM\CurrentControlSet\Services\WdFilter"/>
          <!-- MpClient.dll exists in the Program Files folder -->
          <bar:FileExists Path="Windows Defender\MpClient.dll" Csidl="38"/>
        </lar:And>
      </upd:IsInstalled>
