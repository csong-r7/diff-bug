        <upd:UninstallNotes>&#x410;&#x43A;&#x442;&#x443;&#x430;&#x43B;&#x438;&#x437;&#x430;&#x446;&#x438;&#x44F;&#x442;&#x430; &#x43D;&#x430; &#x441;&#x43E;&#x444;&#x442;&#x443;&#x435;&#x440;&#x430; &#x43C;&#x43E;&#x436;&#x435; &#x434;&#x430; &#x431;&#x44A;&#x434;&#x435; &#x43E;&#x442;&#x441;&#x442;&#x440;&#x430;&#x43D;&#x435;&#x43D;&#x430;, &#x43A;&#x430;&#x442;&#x43E; &#x438;&#x437;&#x431;&#x435;&#x440;&#x435;&#x442;&#x435; "&#x41F;&#x440;&#x435;&#x433;&#x43B;&#x435;&#x434; &#x43D;&#x430; &#x438;&#x43D;&#x441;&#x442;&#x430;&#x43B;&#x438;&#x440;&#x430;&#x43D;&#x438; &#x430;&#x43A;&#x442;&#x443;&#x430;&#x43B;&#x438;&#x437;&#x430;&#x446;&#x438;&#x438;" &#x432; &#x440;&#x430;&#x437;&#x434;&#x435;&#x43B; "&#x41F;&#x440;&#x43E;&#x433;&#x440;&#x430;&#x43C;&#x438; &#x438; &#x43A;&#x43E;&#x43C;&#x43F;&#x43E;&#x43D;&#x435;&#x43D;&#x442;&#x438;" &#x43D;&#x430; &#x43A;&#x43E;&#x43D;&#x442;&#x440;&#x43E;&#x43B;&#x43D;&#x438;&#x44F; &#x43F;&#x430;&#x43D;&#x435;&#x43B;.</upd:UninstallNotes>
