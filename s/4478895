          &#x5DE;&#x5D4;&#x5D9;&#x5E8;: Internet Explorer 9 &#x5DE;&#x5D1;&#x5E6;&#x5E2; &#x5D0;&#x5EA; &#x5DB;&#x5DC; &#x5D4;&#x5E4;&#x5E2;&#x5D5;&#x5DC;&#x5D5;&#x5EA; &#x5D1;&#x5DE;&#x5D4;&#x5D9;&#x5E8;&#x5D5;&#x5EA;. Internet Explorer 9, &#x5D4;&#x5DE;&#x5D9;&#x5D5;&#x5E2;&#x5D3; &#x5DC;&#x5E0;&#x5E6;&#x5DC; &#x5D0;&#x5EA; &#x5DE;&#x5DC;&#x5D5;&#x5D0; &#x5D4;&#x5D9;&#x5EA;&#x5E8;&#x5D5;&#x5E0;&#x5D5;&#x5EA; &#x5E9;&#x5DC; &#x5D7;&#x5D5;&#x5DE;&#x5E8;&#x5EA; &#x5D4;&#x5DE;&#x5D7;&#x5E9;&#x5D1; &#x5E9;&#x5DC;&#x5DA; &#x5D1;&#x5D0;&#x5DE;&#x5E6;&#x5E2;&#x5D5;&#x5EA; Windows, &#x5DE;&#x5E1;&#x5E4;&#x5E7; &#x5D7;&#x5D5;&#x5D5;&#x5D9;&#x5D5;&#x5EA; &#x5E9;&#x5D9;&#x5DE;&#x5D5;&#x5E9; &#x5DE;&#x5E7;&#x5D9;&#x5E4;&#x5D5;&#x5EA; &#x5E2;&#x5DD; &#x5D2;&#x5E8;&#x5E4;&#x5D9;&#x5E7;&#x5D4; &#x5E2;&#x5E9;&#x5D9;&#x5E8;&#x5D4;, &#x5D4;&#x5DE;&#x5D2;&#x5D9;&#x5D1;&#x5D5;&#x5EA; &#x5D1;&#x5D9;&#x5E2;&#x5D9;&#x5DC;&#x5D5;&#x5EA; &#x5D5;&#x5D1;&#x5DE;&#x5D4;&#x5D9;&#x5E8;&#x5D5;&#x5EA; &#x5D1;&#x5D3;&#x5D5;&#x5DE;&#x5D4; &#x5DC;&#x5D9;&#x5D9;&#x5E9;&#x5D5;&#x5DE;&#x5D9;&#x5DD; &#x5D4;&#x5DE;&#x5E7;&#x5D5;&#x5E8;&#x5D9;&#x5D9;&#x5DD; &#x5D4;&#x5DE;&#x5D5;&#x5EA;&#x5E7;&#x5E0;&#x5D9;&#x5DD; &#x5D1;&#x5DE;&#x5D7;&#x5E9;&#x5D1;.

          &#x5E0;&#x5E7;&#x5D9;: Internet Explorer &#x5DE;&#x5E2;&#x5D1;&#x5D9;&#x5E8; &#x5D0;&#x5EA; &#x5D4;&#x5DE;&#x5D5;&#x5E7;&#x5D3; &#x5DC;&#x5D0;&#x5EA;&#x5E8;&#x5D9; &#x5D4;&#x5D0;&#x5D9;&#x5E0;&#x5D8;&#x5E8;&#x5E0;&#x5D8; &#x5D4;&#x5D7;&#x5D1;&#x5D9;&#x5D1;&#x5D9;&#x5DD; &#x5E2;&#x5DC;&#x5D9;&#x5DA; &#x5E2;&#x5DD; &#x5DE;&#x5E8;&#x5D0;&#x5D4; &#x5E0;&#x5E7;&#x5D9; &#x5D5;&#x5D0;&#x5D6;&#x5D5;&#x5E8; &#x5EA;&#x5E6;&#x5D5;&#x5D2;&#x5D4; &#x5DE;&#x5D5;&#x5E8;&#x5D7;&#x5D1;, &#x5DB;&#x5DA; &#x5E9;&#x5D0;&#x5EA;&#x5E8;&#x5D9; &#x5D4;&#x5D0;&#x5D9;&#x5E0;&#x5D8;&#x5E8;&#x5E0;&#x5D8; &#x5E9;&#x5DC;&#x5DA; &#x5E0;&#x5E8;&#x5D0;&#x5D9;&#x5DD; &#x5D1;&#x5DE;&#x5D9;&#x5D8;&#x5D1;&#x5DD;. &#x5E9;&#x5D9;&#x5DC;&#x5D5;&#x5D1; &#x5D7;&#x5DC;&#x5E7; &#x5D5;&#x5D0;&#x5D9;&#x5E0;&#x5D8;&#x5D5;&#x5D0;&#x5D9;&#x5D8;&#x5D9;&#x5D1;&#x5D9; &#x5E2;&#x5DD; Windows Vista &#x5DE;&#x5E1;&#x5E4;&#x5E7; &#x5D2;&#x5D9;&#x5E9;&#x5D4; &#x5D1;&#x5DC;&#x5D7;&#x5D9;&#x5E6;&#x5D4; &#x5D0;&#x5D7;&#x5EA; &#x5DC;&#x5D9;&#x5D9;&#x5E9;&#x5D5;&#x5DE;&#x5D9; &#x5D0;&#x5D9;&#x5E0;&#x5D8;&#x5E8;&#x5E0;&#x5D8; &#x5D4;&#x5DE;&#x5D5;&#x5E6;&#x5DE;&#x5D3;&#x5D9;&#x5DD; &#x5D9;&#x5E9;&#x5D9;&#x5E8;&#x5D5;&#x5EA; &#x5DC;&#x5E9;&#x5D5;&#x5E8;&#x5EA; &#x5D4;&#x5DE;&#x5E9;&#x5D9;&#x5DE;&#x5D5;&#x5EA;.

          &#x5DE;&#x5D4;&#x5D9;&#x5DE;&#x5DF;: Internet Explorer &#x5D4;&#x5D5;&#x5D0; &#x5D4;&#x5D3;&#x5E8;&#x5DA; &#x5D4;&#x5DE;&#x5D4;&#x5D9;&#x5DE;&#x5E0;&#x5D4; &#x5DC;&#x5E9;&#x5D9;&#x5DE;&#x5D5;&#x5E9; &#x5D1;&#x5D0;&#x5D9;&#x5E0;&#x5D8;&#x5E8;&#x5E0;&#x5D8;, &#x5DE;&#x5D0;&#x5D7;&#x5E8; &#x5E9;&#x5D4;&#x5D5;&#x5D0; &#x5DB;&#x5D5;&#x5DC;&#x5DC; &#x5E2;&#x5E8;&#x5DB;&#x5D4; &#x5DE;&#x5E7;&#x5D9;&#x5E4;&#x5D4; &#x5E9;&#x5DC; &#x5D8;&#x5DB;&#x5E0;&#x5D5;&#x5DC;&#x5D5;&#x5D2;&#x5D9;&#x5D5;&#x5EA; &#x5DE;&#x5D5;&#x5DB;&#x5DC;&#x5DC;&#x5D5;&#x5EA; &#x5E9;&#x5DC; &#x5D0;&#x5D1;&#x5D8;&#x5D7;&#x5D4;, &#x5E4;&#x5E8;&#x5D8;&#x5D9;&#x5D5;&#x5EA; &#x5D5;&#x5D0;&#x5DE;&#x5D9;&#x5E0;&#x5D5;&#x5EA; &#x5D4;&#x5E9;&#x5D5;&#x5DE;&#x5E8;&#x5D5;&#x5EA; &#x5E2;&#x5DC; &#x5D1;&#x5D8;&#x5D7;&#x5D5;&#x5E0;&#x5DA; &#x5D5;&#x5DE;&#x5E2;&#x5E0;&#x5D9;&#x5E7;&#x5D5;&#x5EA; &#x5DC;&#x5DA; &#x5D7;&#x5D5;&#x5D5;&#x5D9;&#x5D9;&#x5EA; &#x5D2;&#x5DC;&#x5D9;&#x5E9;&#x5D4; &#x5DC;&#x5DC;&#x5D0; &#x5D4;&#x5E4;&#x5E8;&#x5E2;&#x5D5;&#x5EA;.

          &#x5D9;&#x5DB;&#x5D5;&#x5DC;&#x5EA; &#x5E4;&#x5E2;&#x5D5;&#x5DC;&#x5D4; &#x5D4;&#x5D3;&#x5D3;&#x5D9;&#x5EA;: &#x5D4;&#x5D9;&#x5D9;&#x5E9;&#x5D5;&#x5DD; &#x5DB;&#x5D5;&#x5DC;&#x5DC; &#x5EA;&#x5DE;&#x5D9;&#x5DB;&#x5D4; &#x5E2;&#x5D1;&#x5D5;&#x5E8; HTML5 &#x5D5;&#x5EA;&#x5E7;&#x5E0;&#x5D9; &#x5D0;&#x5D9;&#x5E0;&#x5D8;&#x5E8;&#x5E0;&#x5D8; &#x5DE;&#x5D5;&#x5D3;&#x5E8;&#x5E0;&#x5D9;&#x5D9;&#x5DD; &#x5E9;&#x5EA;&#x5D5;&#x5DB;&#x5E0;&#x5E0;&#x5D5; &#x5DC;&#x5E0;&#x5E6;&#x5DC; &#x5D0;&#x5EA; &#x5D4;&#x5D9;&#x5EA;&#x5E8;&#x5D5;&#x5E0;&#x5D5;&#x5EA; &#x5E9;&#x5DC; &#x5D4;- GPU; &#x5E4;&#x5D9;&#x5E8;&#x5D5;&#x5E9; &#x5D4;&#x5D3;&#x5D1;&#x5E8; &#x5D4;&#x5D5;&#x5D0; &#x5E9;&#x5D0;&#x5D5;&#x5EA;&#x5D5; &#x5E1;&#x5D9;&#x5DE;&#x5D5;&#x5DF; &#x5DC;&#x5D0; &#x5E8;&#x5E7; &#x5E4;&#x5D5;&#x5E2;&#x5DC; &#x5D1;&#x5DB;&#x5DC; &#x5D7;&#x5DC;&#x5E7;&#x5D9; &#x5D4;&#x5D0;&#x5D9;&#x5E0;&#x5D8;&#x5E8;&#x5E0;&#x5D8;, &#x5D0;&#x5DC;&#x5D0; &#x5D2;&#x5DD; &#x5E4;&#x5D5;&#x5E2;&#x5DC; &#x5D1;&#x5DE;&#x5D4;&#x5D9;&#x5E8;&#x5D5;&#x5EA; &#x5E8;&#x5D1;&#x5D4; &#x5D9;&#x5D5;&#x5EA;&#x5E8; &#x5D5;&#x5DE;&#x5E1;&#x5E4;&#x5E7; &#x5D7;&#x5D5;&#x5D5;&#x5D9;&#x5D4; &#x5E2;&#x5E9;&#x5D9;&#x5E8;&#x5D4; &#x5D9;&#x5D5;&#x5EA;&#x5E8; &#x5D1;&#x5D0;&#x5DE;&#x5E6;&#x5E2;&#x5D5;&#x5EA; Windows &#x5D5;- Internet Explorer 9.
