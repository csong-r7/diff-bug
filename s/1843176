      </upd:Prerequisites>
    </upd:Relationships>
    <upd:ApplicabilityRules>
      <upd:IsInstalled>
        <lar:Not>
          <lar:And>
            <bar:RegSz Key="HKEY_LOCAL_MACHINE" Subkey="HARDWARE\DESCRIPTION\System\CentralProcessor\0" Value="VendorIdentifier" Comparison="Contains" Data="AuthenticAMD"/>
