    <upd:ApplicabilityRules>
      <upd:IsInstalled>
        <lar:And xmlns:lar="http://schemas.microsoft.com/msus/2002/12/LogicalApplicabilityRules">
          <bar:WmiQuery xmlns:bar="http://schemas.microsoft.com/msus/2002/12/BaseApplicabilityRules" Namespace="root\cimv2" WqlQuery="SELECT TotalPhysicalMemory FROM Win32_ComputerSystem WHERE TotalPhysicalMemory &gt;= '2147483648'"/>
          <bar:WmiQuery xmlns:bar="http://schemas.microsoft.com/msus/2002/12/BaseApplicabilityRules" Namespace="root\cimv2" WqlQuery="SELECT DeviceID FROM Win32_USBController WHERE (DeviceID LIKE '%VEN[_]10DE&amp;DEV[_]003C%') OR (DeviceID LIKE '%VEN[_]10DE&amp;DEV[_]005B%') OR (DeviceID LIKE '%VEN[_]10DE&amp;DEV[_]0068%') OR (DeviceID LIKE '%VEN[_]10DE&amp;DEV[_]00D8%') OR (DeviceID LIKE '%VEN[_]10DE&amp;DEV[_]00E8%')"/>
        </lar:And>
      </upd:IsInstalled>
