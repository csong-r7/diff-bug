    <upd:LocalizedPropertiesCollection>
      <upd:LocalizedProperties>
        <upd:Language>en</upd:Language>
        <upd:Title>MSE: Product Enabled</upd:Title>
        <upd:Description>Detects whether the Microsoft Security Essentials product has been disabled by either configuration or policy.</upd:Description>
      </upd:LocalizedProperties>
    </upd:LocalizedPropertiesCollection>
    <upd:Relationships>
      <upd:Prerequisites>
