          </lar:Not>
          <lar:Not>
            <bar:RegDword Key="HKEY_LOCAL_MACHINE" Subkey="SOFTWARE\Microsoft\NET Framework Setup\NDP\WU" Value="BlockNetFramework45" Comparison="EqualTo" Data="1"/>
          </lar:Not>
