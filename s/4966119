        <upd:MoreInfoUrl>http://www.microsoft.com/downloads/details.aspx?FamilyId=986F9A8D-AFE7-455A-B78D-0795CBB0E80E&amp;displaylang=en</upd:MoreInfoUrl>
        <upd:SupportUrl>http://support.microsoft.com/select/default.aspx?target=assistance&amp;ln=zh-hk&amp;x=9&amp;y=8</upd:SupportUrl>
      </upd:LocalizedProperties>
